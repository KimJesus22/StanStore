{
  "name": "stanstore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run test:security && next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "i18n:check": "node scripts/check-i18n.js",
    "test": "vitest",
    "test:security": "vitest run src/tests/env.security.test.ts",
    "test:coverage": "vitest run --coverage",
    "test:staged": "vitest related --project=unit --run",
    "docker:up": "docker compose up --build -d",
    "docker:down": "docker compose down",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "cross-env ANALYZE=true next build --webpack",
    "update-types": "npx supabase gen types typescript --project-id \"mi-id\" > src/types/database.types.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "npm run test:staged"
    ]
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@sentry/nextjs": "^10.38.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.95.3",
    "@tanstack/react-query": "^5.90.21",
    "@types/canvas-confetti": "^1.9.0",
    "@xenova/transformers": "^2.17.2",
    "canvas-confetti": "^1.9.4",
    "cookies-next": "^6.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.34.2",
    "gray-matter": "^4.0.3",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.574.0",
    "next": "16.1.6",
    "next-intl": "^4.8.3",
    "next-swagger-doc": "^0.4.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "^7.71.1",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.7.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "stripe": "^20.3.1",
    "styled-components": "^6.3.10",
    "swagger-ui-react": "^5.31.1",
    "zod": "^4.3.6",
    "zod-to-json-schema": "^3.25.1",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.1",
    "@chromatic-com/storybook": "^5.0.1",
    "@next/bundle-analyzer": "^16.1.6",
    "@playwright/test": "^1.58.2",
    "@storybook/addon-a11y": "^10.2.10",
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/addon-onboarding": "^10.2.10",
    "@storybook/addon-vitest": "^10.2.10",
    "@storybook/nextjs-vite": "^10.2.10",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/stripe": "^8.0.417",
    "@types/styled-components": "^5.1.36",
    "@types/swagger-ui-react": "^5.18.0",
    "@vitejs/plugin-react": "^5.1.4",
